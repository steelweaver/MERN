setlocal EnableExtensions EnableDelayedExpansion & set "OUTTEXTFILE=juniper.bat" & del %OUTTEXTFILE% & echo powershell.exe -command "Get-NetAdapterBinding -Name '*' -DisplayName 'Juniper Network Service' ; Disable-NetAdapterBinding -name '*' -DisplayName 'Juniper Network Service' ; Get-NetAdapterBinding -Name '*' -DisplayName 'Juniper Network Service'">>"%OUTTEXTFILE%" & powershell.exe -command "$cred = Get-Credential -UserName 'EMISUPPORT' -Message ' ' ; Start-Process 'cmd' -Credential $cred -ArgumentList ('/k cd {0} & juniper.bat ' -f $PSScriptRoot)"
